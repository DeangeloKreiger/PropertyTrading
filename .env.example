# Network Configuration
PRIVATE_KEY=your_private_key_here
SEPOLIA_RPC_URL=https://sepolia.infura.io/v3/YOUR_INFURA_KEY

# Contract Deployment
DEPLOYER_ADDRESS=0x0000000000000000000000000000000000000000

# Gateway Contract Configuration (NEW)
# Number of pauser addresses (should be set to n_kms + n_copro)
# where n_kms = number of registered KMS nodes
# and n_copro = number of registered coprocessors
NUM_PAUSERS=3

# Pauser addresses (add as many as NUM_PAUSERS)
PAUSER_ADDRESS_0=0x0000000000000000000000000000000000000001
PAUSER_ADDRESS_1=0x0000000000000000000000000000000000000002
PAUSER_ADDRESS_2=0x0000000000000000000000000000000000000003

# KMS Generation Configuration (NEW)
# Renamed from kmsManagement to kmsGeneration
KMS_GENERATION=1

# Gateway Contract Address (NEW)
GATEWAY_CONTRACT_ADDRESS=0x0000000000000000000000000000000000000000

# Deprecated Environment Variables (DO NOT USE)
# PAUSER_ADDRESS (replaced by PAUSER_ADDRESS_[0-N])
# KMS_MANAGEMENT_CONTRACT_ADDRESS (renamed to KMS_GENERATION_CONTRACT_ADDRESS)

# New Naming Convention (v2.0)
KMS_GENERATION_CONTRACT_ADDRESS=0x0000000000000000000000000000000000000000

# Etherscan Verification
ETHERSCAN_API_KEY=your_etherscan_api_key
